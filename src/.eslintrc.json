{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false
  },
  "extends": "eslint:recommended",
  "env": {
    "browser": true
  },
  "globals": {
    "jQuery": false,
    "JSON": false,
    "wp": false,
    "slidebars": false,
    "google": false
  },
  "rules": {
    "strict": 0,
    "no-console": 0
  }
}